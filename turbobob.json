{
	"for_description_of_this_file_see": "https://github.com/function61/turbobob",
	"version_major": 1,
	"project_name": "hellohttp",
	"builders": [
		{
			"name": "default",
			"dockerfile_path": "build-default.Dockerfile",
			"mount_destination": "/go/src/github.com/joonas/fi/hellohttp",
			"dev_command": [
				"bash"
			],
			"dev_ports": ["8012:80"],
			"pass_envs": [],
			"contextless_build": true
		}
	],
	"docker_images": [
		{
			"image": "joonas/hellohttp",
			"dockerfile_path": "Dockerfile",
			"auth_type": "creds_from_env"
		}
	]
}
